{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "no-nested-ternary": 0,
    "default-case": 0,
    "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true } }],
    "no-console": 0,
    "no-param-reassign": [2, { "props": false }],
    "react/prefer-stateless-function": 0,
  }
}
